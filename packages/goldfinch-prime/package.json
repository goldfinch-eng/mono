    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha-no-only": "^1.1.1",
