{
  "name": "@goldfinch-eng/goldfinch-prime",
  "version": "0.1.0",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "description": "A lending protocol built on the blockchain",
  "main": "index.ts",
  "scripts": {
    "start": "yarn hardhat node --hostname 0.0.0.0 --export-all deployments/all_dev.json",
    "test": "NODE_OPTIONS=--unhandled-rejections=strict NODE_ENV=test yarn hardhat test $(find test/ -name '*.test.ts' | grep -v mainnet_forking) --show-stack-traces",
    "test:coverage": "NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test yarn hardhat coverage",
    "test:unfiltered": "NODE_OPTIONS=--unhandled-rejections=strict NODE_ENV=test yarn hardhat test --show-stack-traces",
    "test:mainnet-forking": "NODE_OPTIONS=--unhandled-rejections=strict HARDHAT_FORK_CHAIN_ID=8453 NODE_ENV=test yarn hardhat test $(find test/mainnet_forking/ -name '*.test.ts') --show-stack-traces",
    "test:mainnet-forking:foundry": "source ./scripts/setupFoundryMainnetForkingEnv.sh && FOUNDRY_PROFILE=mainnet-forking forge test",
    "test:tenderly": "NODE_OPTIONS=--unhandled-rejections=strict HARDHAT_FORK_CHAIN_ID=42161 NODE_ENV=test yarn hardhat test $(find test/mainnet_forking/ -name '*.test.ts') --show-stack-traces --network localhost",
    "build": "yarn hardhat compile",
    "pre-commit": "yarn lint-staged",
    "lint": "tsc --noEmit && eslint --resolve-plugins-relative-to=. . && solhint 'contracts/**/*.sol'",
    "lint:fix": "eslint --resolve-plugins-relative-to=. . --fix && prettier . --write && solhint 'contracts/**/*.sol' --fix",
    "verify": "env-cmd -f ../../.env.local yarn hardhat etherscan-verify --api-key $ETHERSCAN_API_KEY --solc-input --network",
    "advance-time": "yarn hardhat run ./blockchain_scripts/advanceTime.ts --network localhost",
    "generate-manifest": "yarn hardhat run ./blockchain_scripts/deployHelpers/generateHardhatUpgradesManifest.ts --network mainnet",
    "verify-contracts-access-control": "yarn hardhat run ./blockchain_scripts/verifyContractsAccessControl.ts --network mainnet",
    "tsc": "tsc"
  },
  "lint-staged": {
    "*.(js(x)?|ts(x)?)": [
      "prettier --write",
      "eslint --resolve-plugins-relative-to=. --fix"
    ],
    "*.sol": [
      "prettier --write",
      "solhint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenZeppelin/starter-kit"
  },
  "keywords": [
    "openzeppelin",
    "truffle",
    "react",
    "dapp",
    "infura",
    "metamask",
    "web3",
    "ganache",
    "web3js",
    "ethereum"
  ],
  "author": "The Goldfinch Community <engineering@goldfinch.finance>",
  "license": "MIT",
  "homepage": "https://github.com/goldfinch-eng/goldfinch-prime/README.md",
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-verify": "^2.0.12",
    "@openzeppelin/contracts": "^5.1.0",
    "@openzeppelin/contracts-upgradeable": "^5.1.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@openzeppelin/upgrades": "^2.8.0",
    "@truffle/contract": "^4.6.9",
    "@typechain/web3-v1": "^3.1.0",
    "@uniswap/lib": "^4.0.1-alpha",
    "bn.js": "^5.1.3",
    "commander": "^8.2.0",
    "contract-proxy-kit": "^2.2.0-alpha.1",
    "csv-parse": "^5.0.3",
    "csv-writer": "^1.6.0",
    "defender-relay-client": "^1.21.3",
    "dotenv": "^8.2.0",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.0.10",
    "lodash": "^4.17.21",
    "mocha-each": "^2.0.1",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@gnosis.pm/safe-core-sdk": "^0.3.1",
    "@goldfinch-eng/utils": "^0.0.1",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/cli": "^2.6.0",
    "@openzeppelin/hardhat-upgrades": "^1.21.0",
